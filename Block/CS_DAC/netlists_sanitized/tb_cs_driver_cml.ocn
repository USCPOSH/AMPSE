simulator( 'spectre )
design(	 "/home/shiyusu1/workarea_TSMC65_9M/simulation/tb_cs_driver_cml/spectre/schematic/netlist/netlist")
resultsDir( "/home/shiyusu1/workarea_TSMC65_9M/simulation/tb_cs_driver_cml/spectre/schematic" )
modelFile( 
    '("/shares/shiyusu1/workarea_TSMC65_9M/tsmcN65_mc_shares.scs" "TT")
)
analysis('tran ?stop "1n"  ?errpreset "moderate"  )
desVar(	  "l_cs_driver" 400n	)
desVar(	  "l_sw_driver" 60n	)
desVar(	  "m_cs_driver" 12	)
desVar(	  "m_sw_driver" 20	)
desVar(	  "w_cs_driver" 8u	)
desVar(	  "w_sw_driver" 3u	)
desVar(	  "fs" 20G	)
desVar(	  "l_cs" 685n	)
